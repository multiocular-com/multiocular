{
  "name": "multiocular",
  "version": "0.3.0",
  "description": "Review dependencies changes to prevent supply chain attack",
  "type": "module",
  "engines": {
    "node": "^22.0.0 || >=24.0.0"
  },
  "keywords": [
    "security",
    "supply chain attack",
    "dependencies",
    "npm",
    "pnpm"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multiocular-com/multiocular.git"
  },
  "bin": "./server/bin.ts",
  "exports": {
    "./server": "./server/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "FORCE_COLOR=1 pnpm run /^test:/",
    "start": "pnpm run /^start:/",
    "build": "rm -Rf dist && pnpm run /^build:/",
    "visual": "storybook dev -c web/.storybook -p 6006 --no-open --host 0.0.0.0",
    "format": "pnpm run /^format:/",
    "update-env": "node ./scripts/update-env.ts",
    "build-visual": "NODE_ENV=test storybook build -c web/.storybook",
    "test:real": "node --run build && TEST_BIN=./dist/server/bin.js pnpm bnt",
    "test:js": "eslint --cache --concurrency auto .",
    "test:css": "stylelint **/*.css **/*.svelte",
    "test:types": "svelte-check",
    "test:audit": "pnpm audit --prod",
    "test:web": "node --run build:web",
    "start:web": "vite web/ --host 0.0.0.0",
    "start:server": "node --watch ./server/bin.ts --web --commit ffe6ff",
    "build:web": "vite build web/",
    "build:server": "node ./scripts/build-server.ts",
    "format:js": "eslint --fix --cache --concurrency auto .",
    "format:prettier": "prettier --experimental-cli --write ."
  },
  "dependencies": {
    "@logux/server": "^0.14.0",
    "@npmcli/arborist": "^9.1.3",
    "libnpmdiff": "^8.0.6",
    "nanostores": "^1.0.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@fontsource-variable/martian-mono": "^5.2.6",
    "@fontsource/jacquard-12": "^5.2.6",
    "@logux/actions": "^0.5.0",
    "@logux/client": "^0.22.0",
    "@logux/eslint-config": "56.1.0",
    "@logux/stylelint-config": "^0.21.0",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@storybook/addon-themes": "^9.1.4",
    "@storybook/svelte": "^9.1.4",
    "@storybook/svelte-vite": "^9.1.4",
    "@sveltejs/vite-plugin-svelte": "^6.1.4",
    "@types/libnpmdiff": "2.0.3",
    "@types/node": "24.3.0",
    "@types/npmcli__arborist": "^6.3.1",
    "@types/ws": "8.18.1",
    "actions-up": "^1.3.0",
    "arborist": "link:@types/npmcli/arborist",
    "autoprefixer": "^10.4.21",
    "better-node-test": "0.7.2",
    "eslint": "9.34.0",
    "eslint-plugin-svelte": "3.11.0",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "keyux": "^0.11.3",
    "nano-staged": "0.8.0",
    "postcss": "^8.5.6",
    "postcss-dark-theme-class": "^2.0.0",
    "postcss-html": "1.8.0",
    "postcss-load-config": "^6.0.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "react": "^19.1.1",
    "storybook": "^9.1.4",
    "stylelint": "16.23.1",
    "svelte": "5.38.6",
    "svelte-check": "4.3.1",
    "svelte-eslint-parser": "1.3.1",
    "svgo": "4.0.0",
    "ts-blank-space": "0.6.2",
    "typescript": "5.9.2",
    "vite": "^7.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "husky"
    ]
  }
}
