{
  "*.{html,md,json}": "prettier --experimental-cli --write",
  "*.{js,ts,cjs}": [
    "prettier --experimental-cli --write",
    "eslint --fix --concurrency auto"
  ],
  "*.css": ["prettier --experimental-cli --write", "stylelint --fix"],
  "*.svelte": [
    "prettier --experimental-cli --write",
    "stylelint --fix",
    "eslint --fix --concurrency auto"
  ],
  "*.svg": "svgo"
}
